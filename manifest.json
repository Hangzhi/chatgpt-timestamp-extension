{
  "manifest_version": 3,
  "name": "ChatGPT Timestamp",
  "version": "0.1",
  "description": "Displays timestamps for ChatGPT messages",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://chat.openai.com/*", "https://chatgpt.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle",
      "world": "MAIN"
    }
  ],
  "permissions": []
}
